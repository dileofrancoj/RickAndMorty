ort { schema } from \"./schema\";\n\nconst setInitialState = { username: \"\", password: \"\" };\n\nconst Login = () => {\n  const [user, setUser] = useState(setInitialState);\n  const { auth, authenticate } = useContext(AuthContext);\n\n  const history = useHistory();\n  useEffect(() => {\n    if (!!getAuthStorage()) history.push(\"/dashboard\");\n  }, [auth]);\n\n  const { register, handleSubmit, errors } = useForm({\n    resolver: yupResolver(schema),\n  });\n\n  const handlerUser = (e) => {\n    setUser({\n      ...user,\n      [e.target.name]: e.target.value,\n    });\n  };\n  const login = () => {\n    const { username, password } = user;\n    authenticate(username, password);\n    history.push(\"/dashboard\");\n  };\n  return (\n    <Row className=\"justify-content-center vh-100 align-items-center m-0 p-0\">\n      <Col md={4} sm={12}>\n        <Form onSubmit={handleSubmit(login)}>\n          <Form.Group>\n            <Form.Label>Usuario *</Form.Label>\n            <Form.Control\n              ref={register}\n              type=\"text\"\n              name=\"username\"\n              onChange={handlerUser}\n              placeholder=\"Ingresa tu usuario\"\n            />\n            <label className=\"text-danger\">{errors.username?.message}</label>\n          </Form.Group>\n          <Form.Group>\n            <Form.Label>Password *</Form.Label>\n            <Form.Control\n              type=\"password\"\n              ref={register}\n              name=\"password\"\n              onChange={handlerUser}\n              placeholder=\"**********\"\n            />\n          </Form.Group>\n          <Button type=\"submit\" variant=\"primary\" className=\"mt-3 w-100\">\n            Ingresar\n          </Button>\n        </Form>\n      </Col>\n    </Row>\n  );\n};\n\nexport default Login;\n","/Users/francodileo/Desktop/2021/practicas/reactLogin/src/pages/Dashboard/Dashboard.js",["153"],"/Users/francodileo/Desktop/2021/practicas/reactLogin/src/pages/Login/schema.js